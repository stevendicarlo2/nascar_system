{
    "name": "NASCAR Fantasy football scoring",
    "version": "1.3",
    "description": "A better way to play fantasy",
    "permissions": ["storage", "webNavigation", "cookies", "*://fantasy.espn.com/", "*://fantasy.espn.com", "tabs"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://fantasy.espn.com/football/league*"],
        "css": [
          "css/datatables.min.css",
          "css/myBootstrap.css", 
          "jquery-ui-1.12.1/jquery-ui.min.css", 
          "jquery-ui-1.12.1/jquery-ui.structure.min.css", 
          "jquery-ui-1.12.1/jquery-ui.theme.min.css",
          "css/style.css"
        ],
        "js": [ 
          "ButtonToolbar/ButtonItem.js",
          "ButtonToolbar/ButtonGroup.js",
          "ButtonToolbar/ButtonToolbar.js",
          "ScoreDataFilter/TeamFilter.js",
          "ScoreDataFilter/WeekRangeFilter.js",
          "ScoreDataFilter/ScoreTypeFilter.js",
          "ScoreDataFilter/ScoreDataFilter.js",          
          "ScoringChart/ScoringChart.js", 
          "WeeklyBreakdownTable/WeeklyTableToolbar.js", 
          "WeeklyBreakdownTable/WeeklyBreakdownTable.js", 
          "WeeklyBreakdownTable/WeeklyBreakdownContainer.js", 
          "NascarScoringDisplayer/NascarScoringDisplayer.js",
          "core/inject_link.js",  
          "jquery/jquery-3.4.1.min.js", 
          "datatables/datatables.min.js", 
          "d3/d3.min.js", 
          "chart/Chart.min.js", 
          "jquery-ui-1.12.1/jquery-ui.min.js"
        ]
      }
    ],
    "manifest_version": 2
  }