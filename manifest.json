{
    "name": "NASCAR Fantasy football scoring",
    "version": "1.1",
    "description": "A better way to play fantasy",
    "permissions": ["storage", "declarativeContent", "webNavigation", "cookies", "*://fantasy.espn.com/", "*://fantasy.espn.com", "tabs"],
    "background": {
      "scripts": ["jquery-3.4.1.min.js", "background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://fantasy.espn.com/football/league*"],
        "js": ["inject_link.js"]
      }
    ],
    "manifest_version": 2
  }